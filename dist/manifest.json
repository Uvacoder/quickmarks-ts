{
  "name": "Quickmarks",
  "version": "0.91",
  "manifest_version": 2,
  "description": "Simply type 'b {keyword}' in omnibox to open bookmark in current tab.", 
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "background": {
    "script": "js/background.js"
  },
  "omnibox": {"keyword": "b"},
  "permissions": [
    "bookmarks",
    "tabs",
    "<all_urls>"
  ],
  "minimum_chrome_version": "38" // Map support
}
